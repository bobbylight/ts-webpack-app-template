{
  "name": "ts-webpack-app-template",
  "version": "0.5.0",
  "description": "A seed project for webapps using TypeScript and webpack.",
  "main": "/",
  "scripts": {
    "clean": "rimraf dist",
    "build": "cross-env NODE_ENV=production webpack --config webpack/webpack.config.js",
    "watch": "cross-env NODE_ENV=dev webpack --config webpack/webpack.config.js --watch",
    "dev": "cross-env NODE_ENV=dev webpack-dev-server --config ./webpack/webpack.config.js",
    "test": "cross-env NODE_ENV=test karma start"
  },
  "author": "Robert Futrell <robert.e.futrell@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "3.3.7",
    "font-awesome": "4.7.0"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.47",
    "cross-env": "4.0.0",
    "css-loader": "0.28.0",
    "file-loader": "0.11.1",
    "html-webpack-plugin": "2.28.0",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "phantomjs-prebuilt": "2.1.14",
    "raw-loader": "0.5.1",
    "rimraf": "2.6.1",
    "style-loader": "0.16.1",
    "ts-loader": "2.0.3",
    "tslint": "5.1.0",
    "tslint-loader": "3.5.3",
    "typescript": "2.3.2",
    "typings": "2.1.1",
    "url-loader": "0.5.8",
    "webpack": "2.4.1",
    "webpack-dev-server": "2.4.5"
  }
}
